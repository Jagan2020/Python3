import csv

with open('/etc/passwd', newline = '') as prfile, open('/home/mana/Work/pass.csv', 'w') as pwfile:
    csvreader = csv.reader(prfile, delimiter = ':')
    writer = csv.writer(pwfile, delimiter = ',',quoting = csv.QUOTE_MINIMAL)
    
    for x in csvreader:
        writer.writerow(x)    
