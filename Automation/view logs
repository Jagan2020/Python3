## HOW TO VIEW LOGS IN PARTICUALAR TIME WITH PARTICUALR TEXT ##

from datetime import datetime,timedelta
from pathlib import Path
import re

inpm = int(input('Enter Minutes:'))
inpt = input('Enter Search Text:')
comp = re.compile(r''+str(inpm)+'.*'+inpt+'')

now = datetime.now()
s = [(now - timedelta(minutes=x)).strftime("%b %e %H:%M") 
     for x in range(inp)]
logs = Path('/var/log/syslog').read_text().splitlines()
for line in logs:
    if comp.search(line):
        print(line)
