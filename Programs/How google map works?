import bisect 

a = [[85.319769, 27.7545],
[85.328429, 27.7495],
[85.329873, 27.742],
[85.337089, 27.7445],
[85.347193, 27.757],
[85.34142, 27.772],
[85.329873, 27.772],
[85.321212, 27.767],
[85.319769, 27.7545]]

def chk(arg):
    idx = bisect.bisect(a, arg)
    return idx

chk([85.3211, 27.7522])
