from itertools import permutations
from functools import reduce
from operator import add

lst_nums = [1, 2, 17, 14, 5, 3, 7, 12, 3, 8, 4, 9]
sum = 15

for i in range(2,len(lst_nums)):
    x = list(permutations(nums, i))
    for y in x:
        tmp = reduce(add, y)
        if tmp == sum:
            print(*y)
            break
